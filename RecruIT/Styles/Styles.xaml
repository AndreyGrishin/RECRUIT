<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="using:Microsoft.Toolkit.Uwp.UI.Controls">

    <DataTemplate x:Key="EmployeesTemplate">
        <Grid
            Height="280"
            MinHeight="250"
            Background="White"
            BorderBrush="#FF2079D3"           
            BorderThickness="3" MinWidth="250">
            <StackPanel Margin="5,10,5,5" Orientation="Vertical">
                <Ellipse
                    Width="100"
                    Height="100"
                    Margin="20,0,27,0"
                    VerticalAlignment="Top"
                    Stroke="#FF449EDC"
                    StrokeThickness="3">
                    <Ellipse.Fill>
                        <ImageBrush ImageSource="{Binding PhotoPath}" Stretch="UniformToFill" />
                    </Ellipse.Fill>
                </Ellipse>
                <StackPanel Margin="0,8,0,0" Orientation="Horizontal">
                    <TextBlock Text="Имя:" />
                    <TextBlock
                        Margin="10,0,0,0"
                        Foreground="#FF858585"
                        Text="{Binding FirstName}" Width="140" />
                </StackPanel>
                <StackPanel Margin="0,8,0,0" Orientation="Horizontal">
                    <TextBlock Text="Фамилия:" />
                    <TextBlock
                        Margin="10,0,0,0"
                        Foreground="#FF858585"
                        Text="{Binding LastName}" Width="102" />
                </StackPanel>
                <StackPanel Margin="0,8,0,0" Orientation="Horizontal">
                    <TextBlock Text="Должность:" />
                    <TextBlock
                        Margin="10,0,0,0"
                        Foreground="#FF858585"
                        Text="{Binding PostName}" TextWrapping="Wrap" Width="94" />
                </StackPanel>

            </StackPanel>

        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="HeaderTemplate">
        <StackPanel>
            <TextBlock
                FontFamily="Arial"
                FontSize="18"
                Text="{Binding}"
                TextWrapping="WrapWholeWords" />
        </StackPanel>
    </DataTemplate>

    <Style x:Key="MaskedTextBoxStyle" TargetType="TextBox">
        <Setter Property="Margin" Value="10,0,10,0" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>

    <Style x:Key="TextBoxRegexStyle" TargetType="TextBox">
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>
